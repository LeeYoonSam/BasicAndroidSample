<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/searchMainFragment">

    <fragment
        android:id="@+id/searchMainFragment"
        android:name="com.ys.basicandroid.presentaion.ui.search.main.SearchMainFragment"
        android:label="SearchMainFragment">
        <action
            android:id="@+id/action_searchMainFragment_to_bookDetailFragment"
            app:destination="@id/bookDetailFragment" >
        </action>
    </fragment>

    <fragment
        android:id="@+id/bookDetailFragment"
        android:name="com.ys.basicandroid.presentaion.ui.search.detail.BookDetailFragment"
        android:label="BookDetailFragment"
        tools:layout="@layout/fragment_book_detail">
        <argument
            android:name="bookInfo"
            app:argType="com.ys.basicandroid.domain.model.BookInfoItemViewModel" />
    </fragment>
</navigation>